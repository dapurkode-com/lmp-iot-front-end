{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lpm-itb-front": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lpm-itb-front",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/mdi/css/materialdesignicons.min.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/css/vendor.bundle.base.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/jvectormap/jquery-jvectormap.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/flag-icon-css/css/flag-icon.min.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/owl-carousel-2/owl.carousel.min.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/owl-carousel-2/owl.theme.default.min.css",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/css/style.css",
              "src/styles.scss"
            ],
            "scripts": [
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/js/vendor.bundle.base.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/chart.js/Chart.min.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/progressbar.js/progressbar.min.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/jvectormap/jquery-jvectormap.min.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/vendors/owl-carousel-2/owl.carousel.min.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/off-canvas.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/hoverable-collapse.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/misc.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/settings.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/todolist.js",
              "vendor/corona-free-dark-bootstrap-admin-template/template/assets/js/dashboard.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lpm-itb-front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "lpm-itb-front:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lpm-itb-front:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "lpm-itb-front:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lpm-itb-front:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "lpm-itb-front"
}
